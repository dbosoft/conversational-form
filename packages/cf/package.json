{
  "name": "@dbosoft/conversational-form",
  "version": "2.0.0-alpha.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbosoft/conversational-form"
  },
  "homepage": "https://github.com/dbosoft/conversational-form",
  "type": "module",
  "source": "src/index.ts",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "src/*",
    "styles/*.css",
    "scss/*.scss"
  ],
  "scripts": {
    "dev:wds": "wds --node-resolve --watch",
    "dev:tsup": "tsup --watch",
    "dev:scss": "run-s scss:*",
    "scss:build": "node-sass scss . -o styles",
    "dev": "run-p dev:*",
    "build:tsup": "tsup",
    "build:scss": "node-sass scss . -o styles",
    "watch:scss": "node-sass --watch scss . -o styles",
    "build": "run-s build:*",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@web/dev-server": "^0.1.35",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^15.1.0",
    "tsconfig": "workspace:*",
    "tslib": "^2.5.0",
    "tsup": "^6.1.3",
    "typescript": "^4.9.4"
  }
}